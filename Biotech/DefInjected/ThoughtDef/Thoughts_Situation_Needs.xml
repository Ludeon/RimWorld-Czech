  <NeedLearning.stages.learningdeprived.label>dlouho bez učení</NeedLearning.stages.learningdeprived.label>
  <!-- EN: I want to learn more about all sorts of things. This place is boring! -->
  <NeedLearning.stages.learningdeprived.description>Chci se toho naučit víc o všem možném. Tady je to fakt nuda.</NeedLearning.stages.learningdeprived.description>
  <!-- EN: learning unfulfilled -->
  <NeedLearning.stages.learning_unfulfilled.label>učení nenaplněno</NeedLearning.stages.learning_unfulfilled.label>
