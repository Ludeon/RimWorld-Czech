  </BeamWeapons.generalRules.rulesStrings>
  
  <!-- EN: fishing -->
  <Fishing.label>rybaření</Fishing.label>
  <!-- EN: Place zones where your colonists can go fishing. -->
  <Fishing.description>Umožní ti vyznačit oblast, ve které budou tvoji kolonisti moci rybařit.</Fishing.description>
  <!-- EN: Fishing zone -->
  <Fishing.customUnlockTexts.0>Rybářskou oblast</Fishing.customUnlockTexts.0>
