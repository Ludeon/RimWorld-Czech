<?xml version="1.0" encoding="UTF-8"?>
<LanguageData>
  
  <!-- EN: go frenzy -->
  <Frenzy_Go.label>běžecká posedlost</Frenzy_Go.label>
  <!-- EN: Inspired: Go frenzy -->
  <Frenzy_Go.baseInspectLine>Inspirace: Běžecká posedlost</Frenzy_Go.baseInspectLine>
  <!-- EN: [PAWN_pronoun] will move faster for the next 8 days. -->
  <Frenzy_Go.beginLetter>Následujících 8 dnů se bude rychleji pohybovat.</Frenzy_Go.beginLetter>
  <Frenzy_Go.beginLetterLabel>Běžecká posedlost</Frenzy_Go.beginLetterLabel>
  <!-- EN: {0} is no longer inspired (go frenzy). -->
  <Frenzy_Go.endMessage>{PAWN_nameIndef} už není {PAWN_gender ? inspirován : inspirována} (běžecká posedlost).</Frenzy_Go.endMessage>
  
  <!-- EN: shoot frenzy -->
  <Frenzy_Shoot.label>střelecká posedlost</Frenzy_Shoot.label>
