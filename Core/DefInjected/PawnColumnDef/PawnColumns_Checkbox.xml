<?xml version="1.0" encoding="UTF-8"?>
<LanguageData>
  
  <!-- EN: Follow master while the master is drafted. -->
  <FollowDrafted.headerTip>Následování páníčka, jestliže je v bojovém režimu.</FollowDrafted.headerTip>
  
  <!-- EN: Follow master while the master is doing field work (hunting or taming animals). -->
  <FollowFieldwork.headerTip>Následování páníčka, jestliže provádí práci v terénu (lovení nebo ochočování zvířat).</FollowFieldwork.headerTip>
  
  <!-- EN: Mark animals to be hunted by your hunters. -->
  <Hunt.headerTip>Označit zvířata, která budou lovena tvými lovci.</Hunt.headerTip>
  
  <!-- EN: Release to wild -->
  <ReleaseAnimalToWild.headerTip>Vypustit do volné přírody</ReleaseAnimalToWild.headerTip>
  
  <!-- EN: Slaughter -->
  <Slaughter.headerTip>Porážka</Slaughter.headerTip>
  
  <!-- EN: Sterilize -->
  <Sterile.headerTip>Sterilizace</Sterile.headerTip>
  
  <!-- EN: Tame a wild animal. -->
  <Tame.headerTip>Ochočit divoké zvíře.</Tame.headerTip>
  
</LanguageData>