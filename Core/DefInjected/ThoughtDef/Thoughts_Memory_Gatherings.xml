  <!-- EN: got married to {0} -->
  <GotMarried.stages.got_married_to.label>uzavření sňatku ({0})</GotMarried.stages.got_married_to.label>
  <!-- EN: I got married! It's so wonderful to be starting a new life together! -->
  <GotMarried.stages.got_married_to.description>Řekli jsme si své Ano! Je to tak nádhedné začít nový život společně!</GotMarried.stages.got_married_to.description>
  
  <!-- EN: held concert -->
  <HeldConcert.stages.held_concert.label>{PAWN_gender ? Uspořádal : Uspořádala} koncert</HeldConcert.stages.held_concert.label>
  <!-- EN: They loved me. They really loved me. -->
  <HeldConcert.stages.held_concert.description>Milovali mě. Opravdu mě milovali.</HeldConcert.stages.held_concert.description>
  
  <!-- EN: inspirational speech -->
  <InspirationalSpeech.stages.inspirational_speech.label>inspirativní projev</InspirationalSpeech.stages.inspirational_speech.label>
