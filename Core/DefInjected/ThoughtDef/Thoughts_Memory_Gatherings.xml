  <AttendedParty.stages.attended_party.description>Na tom večírku jsem se skvěle {PAWN_gender ? bavil : bavila}.</AttendedParty.stages.attended_party.description>
  
  <!-- EN: attended wedding -->
  <AttendedWedding.stages.attended_wedding.label>{PAWN_gender ? Zúčastnil : Zúčastnila} se svatby</AttendedWedding.stages.attended_wedding.label>
