  <!-- EN: attended concert -->
  <AttendedConcert.stages.attended_concert.label>{PAWN_gender ? Zúčastnil : Zúčastnila} se koncertu</AttendedConcert.stages.attended_concert.label>
  <!-- EN: I enjoyed listening to that performance. -->
  <AttendedConcert.stages.attended_concert.description>To představení jsem si fakt {PAWN_gender ? užil : užila}.</AttendedConcert.stages.attended_concert.description>
  
