  <!-- EN: Copy settings -->
  <CommandCopyZoneSettingsLabel>Kopírovat nastavení</CommandCopyZoneSettingsLabel>
  <!-- EN: Copy this zone's settings to the clipboard so you can paste them onto another zone. -->
